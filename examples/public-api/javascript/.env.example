# FlowMaestro API Configuration
# Copy this file to .env and fill in your values

# Your FlowMaestro API key (required)
# Get this from Settings > API & Webhooks in the FlowMaestro dashboard
FLOWMAESTRO_API_KEY=fm_live_your_api_key_here

# API base URL (optional, defaults to production)
# Use http://localhost:3001 for local development
FLOWMAESTRO_BASE_URL=https://api.flowmaestro.io

# Example-specific configuration
# These IDs are placeholders - replace with your actual resource IDs

# Workflow ID for basic execution example
WORKFLOW_ID=wf_your_workflow_id

# Agent ID for chatbot example
AGENT_ID=agent_your_agent_id

# Knowledge Base ID for semantic search example
KNOWLEDGE_BASE_ID=kb_your_knowledge_base_id

# Trigger ID for trigger execution example
TRIGGER_ID=trig_your_trigger_id

# Webhook receiver port (for example 06)
WEBHOOK_PORT=3456
