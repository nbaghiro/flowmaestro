# FlowMaestro API Configuration
# Copy this file to .env and fill in your values
# Then run: source .env

# Your FlowMaestro API key (required)
export FLOWMAESTRO_API_KEY="fm_live_your_api_key_here"

# API base URL
export FLOWMAESTRO_BASE_URL="https://api.flowmaestro.io"

# Resource IDs for examples
export WORKFLOW_ID="wf_your_workflow_id"
export AGENT_ID="agent_your_agent_id"
export KNOWLEDGE_BASE_ID="kb_your_knowledge_base_id"
