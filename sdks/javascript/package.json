{
    "name": "@flowmaestro/sdk",
    "version": "0.1.0",
    "description": "Official JavaScript/TypeScript SDK for FlowMaestro",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts --clean",
        "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
        "typecheck": "tsc --noEmit",
        "test": "vitest run",
        "test:watch": "vitest",
        "lint": "eslint src --ext .ts",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "flowmaestro",
        "workflow",
        "automation",
        "ai",
        "agents",
        "sdk"
    ],
    "author": "FlowMaestro Team",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/flowmaestro/flowmaestro.git",
        "directory": "sdks/javascript"
    },
    "bugs": {
        "url": "https://github.com/flowmaestro/flowmaestro/issues"
    },
    "homepage": "https://flowmaestro.io/docs/sdk/javascript",
    "engines": {
        "node": ">=18.0.0"
    },
    "devDependencies": {
        "@types/node": "^22.0.0",
        "tsup": "^8.0.0",
        "typescript": "^5.3.3",
        "vitest": "^1.2.0"
    },
    "peerDependencies": {
        "typescript": ">=4.7.0"
    },
    "peerDependenciesMeta": {
        "typescript": {
            "optional": true
        }
    }
}
