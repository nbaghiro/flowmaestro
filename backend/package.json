        "@temporalio/client": "^1.13.1",
        "@temporalio/common": "^1.13.1",
        "@temporalio/worker": "^1.13.1",
        "@temporalio/workflow": "^1.13.1",
        "@types/nodemailer": "^7.0.3",
        "async-mutex": "^0.5.0",
        "bullmq": "^5.3.0",
        "chart.js": "^4.5.1",
        "cheerio": "^1.0.0",
        "cohere-ai": "^7.19.0",
        "dotenv": "^16.4.1",
        "exceljs": "^4.4.0",
        "fastify": "^5.6.1",
        "ioredis": "^5.9.2",
        "jsonata": "^2.1.0",
