{
    "name": "@flowmaestro/backend",
    "version": "1.0.0",
    "description": "FlowMaestro Backend API and Orchestrator",
    "main": "dist/backend/src/index.js",
    "scripts": {
        "dev": "tsx watch --include ../shared/src src/index.ts",
        "build": "tsc",
        "start": "node dist/backend/src/index.js",
        "test": "jest",
        "test:unit": "jest --selectProjects unit",
        "test:integration": "jest --selectProjects integration",
        "test:e2e": "jest --selectProjects e2e --runInBand",
        "test:coverage": "jest --coverage",
        "test:smoke": "jest --config jest.smoke.config.js",
        "test:changed": "jest --changedSince=HEAD",
        "test:changed:main": "jest --changedSince=main",
        "test:unit:parallel": "jest --selectProjects unit --shard=1/3 & jest --selectProjects unit --shard=2/3 & jest --selectProjects unit --shard=3/3 & wait",
        "test:integration:parallel": "jest --selectProjects integration --shard=1/5 & jest --selectProjects integration --shard=2/5 & jest --selectProjects integration --shard=3/5 & jest --selectProjects integration --shard=4/5 & jest --selectProjects integration --shard=5/5 & wait",
        "test:parallel": "npm run test:unit:parallel && npm run test:integration:parallel",
        "typecheck": "tsc --noEmit",
        "db:migrate": "dotenv -e .env -- node-pg-migrate up --migrations-table flowmaestro.pgmigrations --schema flowmaestro --database-url-var DATABASE_URL",
        "db:migrate:up": "dotenv -e .env -- node-pg-migrate up --migrations-table flowmaestro.pgmigrations --schema flowmaestro --database-url-var DATABASE_URL",
        "db:migrate:down": "dotenv -e .env -- node-pg-migrate down --migrations-table flowmaestro.pgmigrations --schema flowmaestro --database-url-var DATABASE_URL",
        "db:migrate:create": "dotenv -e .env -- node-pg-migrate create --migrations-table flowmaestro.pgmigrations --migration-file-language sql",
        "db:migrate:status": "docker exec flowmaestro-postgres psql -U flowmaestro -d flowmaestro -c 'SELECT * FROM flowmaestro.\"flowmaestro.pgmigrations\" ORDER BY run_on DESC;'",
        "worker": "npm run build && node dist/backend/src/temporal/worker.js",
        "worker:dev": "tsx watch --include ../shared/src src/temporal/worker.ts",
        "worker:start": "tsx src/temporal/worker.ts",
        "db:seed:agent-templates": "tsx scripts/seed-agent-templates.ts",
        "db:seed:workflow-templates": "tsx scripts/seed-workflow-templates.ts",
        "db:seed:personas": "tsx scripts/seed-personas.ts",
        "db:seed:persona-task-templates": "tsx scripts/seed-persona-task-templates.ts",
        "db:seed": "npm run db:seed:workflow-templates && npm run db:seed:agent-templates && npm run db:seed:personas && npm run db:seed:persona-task-templates",
        "db:seed:system-workflows": "tsx scripts/seed-system-workflows.ts"
    },
    "dependencies": {
        "@anthropic-ai/sdk": "^0.67.0",
        "@aws-sdk/client-redshift-data": "^3.985.0",
        "@fastify/cors": "^11.1.0",
        "@fastify/formbody": "^8.0.2",
        "@fastify/jwt": "^10.0.0",
        "@fastify/multipart": "^9.3.0",
        "@fastify/static": "^8.3.0",
        "@fastify/websocket": "^11.2.0",
        "@flowmaestro/ai-sdk": "*",
        "@flowmaestro/shared": "*",
        "@google-cloud/bigquery": "^8.1.1",
        "@google-cloud/logging": "^11.2.1",
        "@google-cloud/monitoring": "^5.3.1",
        "@google-cloud/storage": "^7.7.0",
        "@google/generative-ai": "^0.24.1",
        "@grpc/grpc-js": "^1.14.3",
        "@opentelemetry/api": "^1.9.0",
        "@opentelemetry/exporter-metrics-otlp-grpc": "^0.208.0",
        "@opentelemetry/exporter-trace-otlp-grpc": "^0.208.0",
        "@opentelemetry/instrumentation": "^0.208.0",
        "@opentelemetry/resources": "^2.2.0",
        "@opentelemetry/sdk-metrics": "^2.2.0",
        "@opentelemetry/sdk-node": "^0.208.0",
        "@opentelemetry/sdk-trace-node": "^2.2.0",
        "@opentelemetry/semantic-conventions": "^1.38.0",
        "@react-email/components": "^1.0.1",
