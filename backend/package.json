        "mysql2": "^3.16.3",
        "nanoid": "^5.1.6",
        "node-web-audio-api": "^1.0.4",
        "nodemailer": "^7.0.10",
        "openai": "^6.7.0",
        "pdf-parse": "^2.4.5",
        "pg": "^8.11.3",
        "pgvector": "^0.2.0",
        "playwright": "^1.57.0",
        "react-email": "^5.0.5",
        "redis": "^4.6.13",
        "resend": "^6.5.2",
        "snowflake-sdk": "^2.3.3",
        "sodium-native": "^5.0.9",
        "stripe": "^20.3.1",
        "tesseract.js": "^7.0.0",
        "twilio": "^5.10.7",
        "uuid": "^9.0.1",
        "vm2": "^3.10.0",
        "xml2js": "^0.6.2",
        "zod": "^3.22.4",
        "zod-to-json-schema": "^3.24.6"
    },
    "devDependencies": {
        "@temporalio/testing": "^1.13.1",
        "@testcontainers/postgresql": "^11.12.0",
        "@testcontainers/redis": "^11.12.0",
        "@types/better-sqlite3": "^7.6.13",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.11.16",
        "@types/pdf-parse": "^1.1.5",
        "@types/pg": "^8.11.0",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "@types/snowflake-sdk": "^1.6.24",
        "@types/supertest": "^6.0.3",
        "@types/uuid": "^9.0.8",
        "@types/ws": "^8.18.1",
        "@types/xml2js": "^0.4.14",
        "better-sqlite3": "^12.4.1",
        "dotenv-cli": "^11.0.0",
        "jest": "^29.7.0",
