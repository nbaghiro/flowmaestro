{
    "name": "Parse PDF and Document Flow",
    "description": "Flow parses a base64 PDF and a base64 DOCX provided via input nodes, then outputs both results.",
    "entryPoint": "pdf-input",
    "nodes": {
        "pdf-input": {
            "type": "input",
            "name": "PDF Input",
            "config": {
                "inputType": "text",
                "description": "Base64 PDF content (override with uploaded PDF base64 if available)",
                "defaultValue": "",
                "required": true,
                "outputVariable": "pdfBase64"
            },
            "position": { "x": 40, "y": 120 }
        },
        "doc-input": {
            "type": "input",
            "name": "DOCX Input",
            "config": {
                "inputType": "text",
                "description": "Base64 DOCX content (override with uploaded DOCX base64 if available)",
                "defaultValue": "",
                "required": true,
                "outputVariable": "docBase64"
            },
            "position": { "x": 40, "y": 300 }
        },
        "parse-pdf": {
            "type": "parsePdf",
            "name": "Parse PDF",
            "config": {
                "base64Input": "",
                "ocrEnabled": false,
                "extractTables": false,
                "outputVariable": "parsedPdf"
            },
            "position": { "x": 320, "y": 120 }
        },
        "parse-document": {
            "type": "parseDocument",
            "name": "Parse Document",
            "config": {
                "base64Input": "",
                "fileType": "docx",
                "preserveFormatting": false,
                "extractImages": false,
                "outputVariable": "parsedDoc"
            },
            "position": { "x": 320, "y": 300 }
        },
        "output-final": {
            "type": "output",
            "name": "Output Parsed Results",
            "config": {
                "outputVariable": "combinedResult",
                "format": "json",
                "fields": ["parsedPdf", "parsedDoc"]
            },
            "position": { "x": 560, "y": 210 }
        }
    },
    "edges": [
        { "id": "e0", "source": "pdf-input", "target": "parse-pdf" },
        { "id": "e1", "source": "doc-input", "target": "parse-document" },
        { "id": "e2", "source": "parse-pdf", "target": "output-final" },
        { "id": "e3", "source": "parse-document", "target": "output-final" }
    ]
}
