{
    "name": "@flowmaestro/shared",
    "version": "1.0.0",
    "description": "Shared types and utilities for FlowMaestro",
    "main": "src/index.ts",
    "types": "src/index.ts",
    "exports": {
        ".": "./src/index.ts"
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "clean": "rm -rf dist",
        "typecheck": "tsc --noEmit",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:changed": "vitest run --changed"
    },
    "devDependencies": {
        "@types/uuid": "^10.0.0",
        "typescript": "^5.3.3",
        "vitest": "^1.2.0"
    },
    "dependencies": {
        "uuid": "^13.0.0"
    }
}
